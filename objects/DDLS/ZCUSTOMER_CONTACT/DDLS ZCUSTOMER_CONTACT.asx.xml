<SOURCE>@AbapCatalog.sqlViewName: 'ZBPCONTACT'
@AbapCatalog.compiler.compareFilter: true
@VDM.viewType: #BASIC
@ClientHandling.algorithm: #SESSION_VARIABLE
@AbapCatalog.preserveKey: true
@AccessControl.authorizationCheck: #CHECK
@EndUserText.label: 'Contact Details of the Customer'
@Search.searchable: true


define view ZCUSTOMER_CONTACT as select from but100
inner join kna1 on kna1.kunnr = but100.partner
left outer join knvk on knvk.kunnr = but100.partner
left outer join adr6 on adr6.addrnumber = knvk.adrnp_2 and
                   adr6.persnumber = knvk.prsnr 
                   
left outer join adr2 on adr2.addrnumber = knvk.adrnp_2 and
                   adr2.persnumber = knvk.prsnr               


 {
 
          
      @UI.lineItem: [{ position: 10, importance: #HIGH }]
      @EndUserText.label: 'Customer Number'
      @Consumption.semanticObject: 'BusinessPartner'
 
 key but100.partner ,
 
 @UI.hidden: true
 key but100.rltyp,
 
 @UI.hidden: true
 key but100.dfval,
 
      @Search:{ defaultSearchElement: true, ranking: #HIGH, fuzzinessThreshold: 1 }
      @UI.selectionField.position: 10
      @UI.lineItem: [{ position: 20, importance: #HIGH }]
      @EndUserText.label: 'Customer Name'
 kna1.name1,
 
      @UI.selectionField.position: 20
      @UI.lineItem: [{ position: 30, importance: #HIGH }]
      @EndUserText.label: 'Contact Person Name'
 concat_with_space(knvk.namev, knvk.name1, 1) as contactpersonname,
 
  @UI.hidden: true
 knvk.anred as Salutation,
 
     @UI.lineItem: [{ position: 40, importance: #HIGH }]
      @EndUserText.label: 'Telephone Number'
 adr2.telnr_long,
 
   @UI.lineItem: [{ position: 50, importance: #HIGH }]
      @EndUserText.label: 'Email Address'
 adr6.smtp_addr,
 
 @UI.hidden: true
 kna1.land1,
 
 @UI.hidden: true
 kna1.ort01,
 
 @UI.hidden: true
 kna1.pstlz,
 
 @UI.hidden: true
 kna1.stras
 
 
    
}
//AUNIT TEST - PASS
where rltyp = 'FLCU01'
//AUNIT TEST - FAIL
//where rltyp = 'UKM000'
